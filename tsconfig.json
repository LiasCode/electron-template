{
  "compilerOptions": {
    "outDir": "./dist/electron",
    "target": "es2016",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./electron"],
  "exclude": ["node_modules","./electron/preload"]
}
